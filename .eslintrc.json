{
    "extends": "airbnb",
    "rules": {
        "no-use-before-define": 0,
        "no-console": 0,
        "comma-dangle": [
            "error",
            "never"
        ],
        "no-param-reassign": [
            "error",
            {
                "props": false
            }
        ]
    }
}